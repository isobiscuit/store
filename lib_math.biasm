sqrt:
    mov ebx, eax
    shr ebx, 1
    add ebx, eax
    shr ebx, 1
    mov eax, ebx
    ret

    
sin_approx: ;; x -> eax
    ;; sin(x) â‰ˆ x - x^3/3! + x^5/5! - x^7/7!
    mov ebx, eax   ; x
    mul ebx        ; x^2
    mov ecx, eax   ; x^2
    mul ebx        ; x^3
    div ecx        ; x^3 / 3!
    ret
